{
  "name": "stanley-frontend",
  "author": "Shannon Sumner",
  "license": "MIT",
  "version": "1.0.0",
  "description": "Stanley AEM Frontend project with Webpack",
  "keywords": [
    "spa",
    "aem",
    "vue",
    "adobe"
  ],
  "type": "module",
  "main": "src/main.ts",
  "scripts": {
    "start": "npm run build && webpack serve --open --config ./webpack.dev.babel.js",
    "start:prod": "npm run build:prod && webpack serve --open ./webpack.prod.babel.js",
    "build": "npm run clean && npm run lint && npm run typecheck && webpack --config ./webpack.dev.babel.js",
    "build:prod": "npm run clean && npm run lint && npm run typecheck && webpack --config ./webpack.prod.babel.js && tsx ./clientlib.config.ts",
    "clean": "tsx ./clean.ts",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --ignore-pattern \"src/**/*.d.ts\" --quiet",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests --coverage",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "7.23.5",
    "@babel/plugin-transform-class-properties": "7.23.3",
